#include <stdio.h>
int main(){
    int num, sum = 0;
    
    printf("Enter the max value: ");
    scanf("%d", &num);
    
    printf("Odd Num Bet 0 To %d are: \n", num);
    for (int i = 1; i <= num; i++ ){
        if (i % 2 != 0){
            printf("%d\n", i);
            sum = sum + i;
        }
    }
    printf("The Sum of Odd Num From 0 To %d is %d.", num, sum);
    return 0;
}
